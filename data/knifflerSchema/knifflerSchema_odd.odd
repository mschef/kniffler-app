                                <valItem ident="writingMode: vertical-lr; textOrientation: sideways-right"/>
                                <valItem ident="textOrientation: straight"/>
                                <valItem ident=" textOrientation: upside-down"/>
                                <valItem ident=" writingMode: vertical-lr; textOrientation: sideways-left"/>
                            </valList>
                        </attDef>
                    </attList>
                </elementSpec>
